<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.1" xmlns="http://xmlns.jcp.org/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/persistence http://xmlns.jcp.org/xml/ns/persistence/persistence_2_1.xsd">
  <persistence-unit name="WebAplikacijaWSPU" transaction-type="RESOURCE_LOCAL">
    <provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
    <class>model.Tipprevoza</class>
    <class>model.Radnik</class>
    <class>model.Stavkaprograma</class>
    <class>model.Tipsmestaja</class>
    <class>model.Programputovanja</class>
    <class>model.Adresa</class>
    <class>model.Programiputovanja</class>
    <class>model.Destinacija</class>
    <class>model.Korisnik</class>
    <class>model.Mestoobilaska</class>
    <class>model.Drzava</class>
    <class>model.Zahtevkorisnika</class>
    <class>model.Grad</class>
    <class>model.Potvrdarezervacije</class>
    <exclude-unlisted-classes>false</exclude-unlisted-classes>
    <properties>
      <property name="javax.persistence.jdbc.url" value="jdbc:oracle:thin:@localhost:1521:orcl"/>
      <property name="javax.persistence.jdbc.user" value="fpis"/>
      <property name="javax.persistence.jdbc.driver" value="oracle.jdbc.OracleDriver"/>
      <property name="javax.persistence.jdbc.password" value="fpis"/>
    </properties>
  </persistence-unit>
</persistence>
